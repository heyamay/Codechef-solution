#include <stdio.h>

int main(void) {
	
	int n,i;
	scanf("%d",&n);
	for(i=0;i<n;i++){
	    int x,y;
	    scanf("%d",&x);
	    scanf("%d",&y);
	    if(x<y){
	        printf("%d\n",x);
	    }
	    else if(x>y){
	        printf("%d\n",y);
	    }
	    else{
	        printf("%d\n",x);
	    }
	}
	return 0;
}
